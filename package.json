{
  "name": "abeta-example-01",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/abeta-example-01/test/jest-e2e.json",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./libs/database-type-orm/src/data-source.ts",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create ./libs/database-type-orm/src/migrations",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate ./libs/database-type-orm/src/migrations/migrations -d ./libs/database-type-orm/src/data-source.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./libs/database-type-orm/src/data-source.ts"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "aws-sdk": "^2.1615.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.5.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.2.0",
    "date-fns": "^3.6.0",
    "firebase": "^10.11.1",
    "firebase-admin": "^12.1.0",
    "flat": "^6.0.1",
    "log4js": "^6.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.7",
    "node-mailer": "^0.1.1",
    "onesignal-node": "^3.4.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.3",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@app/helper(|/.*)$": "<rootDir>/libs/helpers/src/$1",
      "^@app/database-type-orm(|/.*)$": "<rootDir>/libs/database-type-orm/src/$1",
      "^@app/jwt-authentication(|/.*)$": "<rootDir>/libs/jwt-authentication/src/$1",
      "^@app/exception-filter(|/.*)$": "<rootDir>/libs/exception-filter/src/$1",
      "^@app/node-mailer(|/.*)$": "<rootDir>/libs/node-mailer/src/$1",
      "^@app/s3-upload(|/.*)$": "<rootDir>/libs/s3-upload/src/$1",
      "^@app/firebase-storage(|/.*)$": "<rootDir>/libs/firebase-storage/src/$1"
    }
  }
}
