{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/abeta-example-01/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/abeta-example-01/tsconfig.app.json"
  },
  "projects": {
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "helper": {
      "type": "library",
      "root": "libs/helpers",
      "entryFile": "index",
      "sourceRoot": "libs/helpers/src",
      "compilerOptions": {
        "tsConfigPath": "libs/helpers/tsconfig.lib.json"
      }
    },
    "database-type-orm": {
      "type": "library",
      "root": "libs/database-type-orm",
      "entryFile": "index",
      "sourceRoot": "libs/database-type-orm/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database-type-orm/tsconfig.lib.json"
      }
    },
    "abeta-example-01": {
      "type": "application",
      "root": "apps/abeta-example-01",
      "entryFile": "main",
      "sourceRoot": "apps/abeta-example-01/src",
      "compilerOptions": {
        "tsConfigPath": "apps/abeta-example-01/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/abeta-example-01"
}
